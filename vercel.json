{
  "version": 2,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.mjs": {
      "maxDuration": 30
    }
  }
}
